      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 4001,
          "hostPort": 4001,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "MONGODB_URI",
          "valueFrom": "/repostly/api/MONGODB_URI"
